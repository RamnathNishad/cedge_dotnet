@model IEnumerable<HelloWorldMVC.Models.Employee>

@{
    ViewData["Title"] = "Welcome";
}

<h1>Welcome</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Ecode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ename)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Salary)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Deptid)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model) {
            Html.RenderPartial("_EmpItemView",item); 
        }
    </tbody>
</table>

@section s1{
    <h3>This is section 1 for menus</h3>
}

@section s2{
    <h3>This is section 2 for side bar</h3>
}